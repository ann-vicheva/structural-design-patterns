<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pp_lab2_struct">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AdapterActivity" />
        <activity android:name=".DecoratorActivity" />
        <activity android:name=".ProxyActivity" />
        <activity android:name=".CompositeActivity" />
        <activity android:name=".BridgeActivity" />
        <activity android:name=".FlyweightActivity" />
        <activity android:name=".FacadeActivity"></activity>
    </application>

</manifest>